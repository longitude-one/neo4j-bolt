{
    "name": "longitude-one/neo4j-bolt",
    "description": "Neo4j Bolt Binary Protocol PHP Driver",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Willemsen",
            "email": "christophe@graphaware.com"
        }
    ],
    "require": {
        "php": ">= 7.3",
        "ext-bcmath": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "symfony/event-dispatcher": "^4.4|^5.0",
        "graphaware/neo4j-common": "^3.4",
        "myclabs/php-enum": "^1.4"
    },
    "autoload": {
      "psr-4": {
          "GraphAware\\Bolt\\": "src/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "GraphAware\\Bolt\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "symfony/stopwatch": "^2.7",
        "behat/behat": "^3.6"
    },
    "minimum-stability": "dev"
}
